<%= turbo_stream.replace "account_privacy"  do%>
    <%=form_with model: @user ,url:privacyupdate_path,method: :patch do |f| %>
      <%= f.select :privacy_status,options_for_select(User.privacy_statuses,   :selected => @user.privacy_status)  %>
      <%= f.submit %>
    <%end%>
<%end%>
<%= render_turbo_stream_flash_message %>