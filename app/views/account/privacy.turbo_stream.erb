<%if @privacy_success %>
  <%= turbo_stream.replace "account_privacy"  do %>
    <%= render partial: "privacy" %>
  <% end %>
  <% flash[:notice] = "Privacy option is successfully updated." %>
<%else %>
  <% flash[:notice] =  "You are not authorized to update" %>
<% end %>

<%= render_turbo_stream_flash_message %>