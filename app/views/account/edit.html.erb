<%= turbo_frame_tag "account_#{current_user.id}" do %>
   <%= form_with model: @user,url:account_path, method: :patch do |f| %>
      <%= f.label :username, placeholder: "username", :class => "form_control"%>
      <%= f.text_field :username %>
      <%= f.label "bio", placeholder: "bio", :class => "form_box"%>
      <%= f.text_field :bio %>
      <%= f.file_field :avatar %>
      <%= f.submit %>    
   <% end %>
   <%= button_to "cancel", cancelaccount_path,method: :get %>
<% end %>