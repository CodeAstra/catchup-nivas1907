<%= turbo_frame_tag "like_#{post.id}" do %>
  <% if like=post.likes.find_by(user_id:current_user) %>
    <%= button_to "❤️", post_like_path( post_id: post, id: like.id ), method: :delete %>
  <%else %>
    <%= button_to "🤍", post_likes_path(post), method: :post %>
  <% end %>
  <p style="margin: 0px">Likes count <%= post.likes_count %></p>
<% end %>