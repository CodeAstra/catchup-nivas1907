<%= button_to "Add Friends",new_friendship_path,method: :get %>
<%= button_to "Pending Requests(#{@pending_list_count})",pending_requests_friendships_path,method: :get %>
<%= form_with url: search_friendships_path, method: :get do |f| %>
   <%= f.text_field :search, value: params[:search]  %>
   <%= f.submit "Search" %>
<% end %>

<% @friends_listeach do |u| %>

    <% if u.avatar.attached? %>
        <img src="<%=url_for(u.avatar)%>" class="avatar" />
        <h1></h1>
    <%end%>
    <%= link_to  (
    u.username||u.email
    ), account_path(u.id) , method: :get , data: { turbo_frame: "_top" } %>

    <h1></h1>
<%end%>