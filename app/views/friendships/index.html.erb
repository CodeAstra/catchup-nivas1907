<%= button_to "Add Friends", new_friendship_path, method: :get %>
<%= button_to "Pending Requests(#{pending_list_count})", pending_requests_friendships_path, method: :get %>
<%= render partial: "search", locals: { search_type: "friends" } %>
<% @friends_list.each do |user| %>
  <%= image_tag url_for(user.avatar), class: "avatar" if user.avatar.attached? %>
  <%= link_to  user.name, account_path(user), method: :get%>
  <h1></h1>
<% end %>